{{#each this}}
<li>
    <h1>{{name}}</h1>
    <img src='{{flag}}' alt='flag' width='340' />
    <h3>Capital:</h3>
    <p>{{capital}}</p>
    <h3>Population:</h3>
    <p> {{population}}</p>
    <ol>
    <h3>Languages:</h3>
    {{#each languages}}
    <li class="item">{{name}}</li>
    {{/each}}
    </ol>
</li>
  

{{/each}}